{
	"env": {
		"node": true,
		"es2021": true,
		"browser": true
	},
	"extends": ["airbnb-base", "plugin:jest/recommended"],
	"parser": "babel-eslint",
	"rules": {
		"no-console": 0,
		"import/extensions": 0,
		"no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }]
	},
	"plugins": ["jest", "babel"]
}
